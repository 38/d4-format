	"version": "0.2.0",
	"configurations": [
	
		{
			"type": "lldb",
			"request": "launch",
			"name": "Cargo launch",
			"cargo": {
				"args": [
					"build",
					"--package=d4tools"
				]
			},
			"program": "target/debug/d4tools",
			"env": {"RUST_LOG": "info",  "RUST_BACKTRACE": "1"},
			"args": ["view", "${workspaceFolder}/fdr.peaks.d4", "chr1:1-59999"],
			"cwd": "${workspaceFolder}" 
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug pyd4",
			"cwd": "${workspaceFolder}/pyd4/build/lib",
			"program": "python3",
			"env": {"RUST_LOG": "info"},
			"args": ["test3.py"]
		},
	]
}